{
        "version": "0.2.0",
        "configurations": [
            {
                "type": "chrome",
                "request": "attach",
                "name": "Attach to Chrome",
                "port": 2000,
                "webRoot": "${workspaceRoot}"
            },
            {
                "type": "node",
                "request": "launch",
                "name": "h5_multiplayer_swb",
                "program": "${workspaceRoot}\\app.js",
                "osx": {
                    "program": "${workspaceRoot}\/app.js"
                }
            },
            {
                "type": "chrome",
                "request": "launch",
                "name": "Launch Chrome",
                "url": "http://localhost:2000",
                "webRoot": "${workspaceRoot}"
            }
        ]
    }